{
    "name": "Post",
    "backendRoute": [{
            "url": "getPost",
            "parameters": ["tmid"]
        },
        {
            "url": "addPost",
            "parameters": ["tmid", "msg", "token"]
        },
        {
            "url": "addComment",
            "parameters": ["post_id", "msg", "token"]
        },
        {
            "url": "deleteComment",
            "parameters": ["cmid", "token"]
        },
        {
            "url": "deletePost",
            "parameters": ["psid", "token"]
        },
        {
            "url": "getLastPost",
            "parameters": ["tmid"]
        }
