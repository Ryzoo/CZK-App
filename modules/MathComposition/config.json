{
    "name": "MathComposition",
    "description": "Dodaje możliwość graficznego ułożenia zawodników na boisku oraz podglądu formy dla danego zestawienia",
    "require": ["StatsManager"],
    "version": "1.0",
    "backendRoute": [{
        "url": "getUsersHelpfulness",
        "parameters": ["teamForm", "otherTeam", "tmid"]
    }],
    "jsControllers": ["compositionController"],
    "frontendRoute": [{
        "url": "teamComposition",
        "templateSrc": "actualTeamComposition.html",
        "controllerName": "compositionController"
    }],
    "anchor": [{
        "destination": "leftMenu",
        "anchorPoint": "Szatnia",
        "anchorWeight": 3,
        "anchorPointIcon": "fa fa-users fa-fw",
        "anchorAdditional": "ng-show=\"user.tmid != ''\"",
        "content": {
            "link": {
                "label": "Skład meczowy",
                "url": "#!/teamComposition"
            }
        }
    }],
    "maxPlayers": 11,
    "colorId": 1
}